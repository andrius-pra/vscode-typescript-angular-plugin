{
    "name": "vscode-typescript-angular-plugin",
    "version": "0.900.0-rc5",
    "icon": "angular.png",
    "description": "Typescript plugin for angular templates",
    "repository": {
        "type": "git",
        "url": "https://github.com/andrius-pra/vscode-typescript-angular-plugin"
    },
    "publisher": "andrius-pra",
    "author": "andrius-pra",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "angular",
        "angular language service",
        "typescript",
        "html",
        "multi-root ready"
    ],
    "main": "extension/dist/extension.js",
    "activationEvents": [
        "onLanguage:html"
    ],
    "contributes": {
        "typescriptServerPlugins": [
            {
                "name": "@angular/language-service",
                "enableForWorkspaceTypeScriptVersions": true,
                "languages": [
                    "html"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./extension",
        "update-vscode": "vscode-install",
        "postinstall": "vscode-install",
        "dist": "tsc -p ./extension/ && vsce package"
    },
    "dependencies": {
        "@angular/language-service": "https://npmpackages.blob.core.windows.net/andrius-pra/language-service-9.0.0-rc.5-experimental.tgz"
    },
    "devDependencies": {
        "typescript": "https://npmpackages.blob.core.windows.net/andrius-pra/typescript-3.6.4-experimental-plugin-external-files.20191113.tgz",
        "vsce": "^1.70.0",
        "vscode": "^1.1.36"
    },
    "peerDependencies": {
        "diff": "^4.0.1"
    }
}
